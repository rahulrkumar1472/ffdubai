generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Booking {
  id        String   @id @default(cuid())
  locale    String
  name      String
  email     String
  phone     String
  date      String
  time      String
  createdAt DateTime @default(now())

  @@index([createdAt])
}
